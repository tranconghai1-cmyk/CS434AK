const express = require("express");
const cors = require("cors");
const app = express();

// Middleware
app.use(cors());
app.use(express.json());

// Routes
app.use("/api/payment", require("./routes/paymentRoutes"));

// Start server
const PORT = 5000;
app.listen(PORT, () => {
  console.log(`Backend cháº¡y port ${PORT}`);
});